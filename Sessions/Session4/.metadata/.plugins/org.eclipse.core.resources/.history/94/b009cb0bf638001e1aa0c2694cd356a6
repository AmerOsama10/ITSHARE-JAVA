package com.itshare.exceptions.example1;

public class Task2 {

	public static void main(String[] args) throws Exception {
		// TODO Auto-generated method stub

		int num1= 10;
		int num2= 0;
		
		
		if(num2 == 0) {
			throw new Exception("no divide by zero");
		}else
			System.out.println(num1/num2);
		
	}

}
