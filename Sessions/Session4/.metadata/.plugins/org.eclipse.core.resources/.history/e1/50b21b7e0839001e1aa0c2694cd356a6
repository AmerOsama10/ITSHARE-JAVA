package com.itshare.files.creation;

import java.io.File;
import java.io.IOException;

public class Test {

	public static void main(String[] args) {

		File object = new File("D:\\work space\\Session4\\Files\\src\\com\\itshare\\files\\creation\\mohamed.txt");
		
		try {
			System.out.println(object.createNewFile());
			System.out.println(object.getAbsoluteFile());
			System.out.println(object.getAbsolutePath());
			System.out.println(object.getName());
			System.out.println(object.getParent());
			System.out.println(object.hashCode());

			
			
			System.out.println("file created successfully");
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}

}
